{
  "name": "pettycash",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "babel ./src -d ./dist",
    "start": "NODE_ENV=production yarn run build && node ./dist/index.js",
    "lint": "eslint 'src/**/*.js' --fix",
    "start:dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "pretest": "NODE_ENV=test yarn migrations",
    "test": "NODE_ENV=test jest",
    "db:rollback": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "migrations": "./node_modules/.bin/sequelize db:migrate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "googleapis": "^43.0.0",
    "graphql": "^14.5.4",
    "lodash": "^4.17.15",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.18.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "sequelize-cli": "^5.5.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/db/migrations",
      "<rootDir>/db/seeders",
      "<rootDir>/db/models/index.js"
    ],
    "coverageReporters": [
      "lcov"
    ]
  }
}
